{
  "name": "tripnus-backend",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": ">=22.0.0 <23.0.0"
  },
"scripts": {
  "dev": "ts-node-dev --respawn src/index.ts",
  "clean": "rm -rf dist",
  "build": "npm run clean && tsc",
  "start": "pm2 start dist/index.js --name TripNusBackend -i max || pm2 reload TripNusBackend",
  "stop": "pm2 stop TripNusBackend",
  
  "docker-build": "docker build -t tripnus-backend -f Dockerfile.prod .",
  "docker-start": "docker run -p 3000:3000 --env-file .env tripnus-backend",
  "docker-stop": "docker ps -q --filter ancestor=tripnus-backend | xargs -r docker stop",
  "docker-rm": "docker ps -a -q --filter ancestor=tripnus-backend | xargs -r docker rm",
  "docker-delete": "npm run docker-stop && npm run docker-rm && docker rmi -f tripnus-backend",
  "docker-content": "docker run --rm -it tripnus-backend sh",
  "docker-logs": "docker logs tripnus-backend",
  "docker-tag": "docker tag tripnus-backend:latest hendrywidyanto/tripnus-backend:latest",
  "docker-push": "docker push hendrywidyanto/tripnus-backend:latest",
  
  "zip": "zip -r zTripNusBackend.zip . -x 'node_modules/**' -x 'dist/**'"
},
  "repository": {
    "type": "git",
    "url": "git@github.com-hendry-personal:Gonzushi/TripNusBackend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@supabase/supabase-js": "^2.49.6",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "pm2": "^6.0.6",
    "pm2-runtime": "^5.4.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.19",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
